{
  "name": "tyrexcad-professional",
  "version": "3.0.0",
  "description": "TyrexCAD Professional - Advanced Web-Based CAD System with 2D/3D capabilities",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 8080 --host",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "rimraf dist node_modules package-lock.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint js/**/*.js --fix",
    "format": "prettier --write \"js/**/*.{js,json,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow npm",
    "postinstall": "npm run prepare-assets",
    "prepare-assets": "node scripts/prepare-assets.js",
    "generate-icons": "node scripts/generate-icons.js",
    "optimize-images": "node scripts/optimize-images.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibrahim119-IN/TyrexCAD.git"
  },
  "keywords": [
    "cad",
    "design",
    "drawing",
    "canvas",
    "2d",
    "3d",
    "engineering",
    "graphics",
    "webgl",
    "three.js",
    "drafting",
    "technical-drawing",
    "geometry",
    "parametric",
    "dwg",
    "dxf"
  ],
  "author": {
    "name": "Ibrahim",
    "email": "ibrahim119.in@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibrahim119-IN/TyrexCAD/issues"
  },
  "homepage": "https://github.com/ibrahim119-IN/TyrexCAD#readme",
  "dependencies": {
    "three": "^0.160.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "clipboard": "^2.0.11",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "vite": "^5.0.11",
    "@vitejs/plugin-legacy": "^5.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.26.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "@types/three": "^0.160.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.4",
    "vitest": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "only-allow": "^1.2.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.5",
    "@rollup/rollup-linux-x64-musl": "^4.9.5",
    "@rollup/rollup-win32-x64-msvc": "^4.9.5",
    "@rollup/rollup-darwin-x64": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "packageManager": "npm@10.2.4",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./js/main.js",
      "require": "./dist/tyrexcad.cjs"
    },
    "./core": "./js/core/TyrexCAD.js",
    "./geometry": "./js/geometry/Geometry.js",
    "./tools": "./js/tools/index.js",
    "./ui": "./js/ui/UI.js"
  },
  "files": [
    "dist",
    "js",
    "css",
    "index.html",
    "README.md",
    "LICENSE"
  ]
}